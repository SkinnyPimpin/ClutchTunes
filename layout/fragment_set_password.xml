<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.bennettdierckman.calclogin.Set_Password">

    <!-- TODO: Update blank fragment layout -->


    <TextView
        android:id="@+id/textView31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="132dp"
        android:text="Password Protection"
        android:textColor="@color/white"
        android:textSize="25sp"
        android:textStyle="bold" />

    <EditText
        android:id="@+id/etPartyPassword"
        android:layout_width="275dp"
        android:layout_height="40dp"
        android:layout_alignLeft="@+id/bNext"
        android:layout_alignStart="@+id/bNext"
        android:layout_below="@+id/textView31"
        android:layout_marginTop="13dp"
        android:background="@color/colorPrimary"
        android:ems="10"
        android:hint="Enter Password"
        android:inputType="textPersonName"
        android:textColor="@color/white" />

    <Button
        android:id="@+id/bNext"
        android:layout_width="275dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/bReturn2Party"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="11dp"
        android:text="Next " />

    <Button
        android:id="@+id/bReturn2Party"
        android:layout_width="275dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/bNext"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/bNext"
        android:layout_marginBottom="38dp"
        android:text="Go Back" />

</RelativeLayout>


